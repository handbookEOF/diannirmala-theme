<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html b:version='2' class='v2' expr:dir='data:blog.languageDirection' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>
  <head>
    <meta expr:content='data:blog.isMobile         ? &quot;width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0&quot;         : &quot;width=1100&quot;' name='viewport'/>
    <b:include data='blog' name='all-head-content'/>
    <title><data:blog.pageTitle/></title>

    <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous"/>

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous"/>

    <!-- Latest compiled and minified JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

    <b:skin>
      <![CDATA[

      ]]>
    </b:skin>

    <b:template-skin>
      <![CDATA[
        html, body{
          width: 100%;
          margin: 0;
          padding: 0;
        }

        .container-fluid{
          width: 100%;
          margin: 0;
          padding: 0;
        }

        .row{
          width: 100%;
          margin: 0;
        }

        .post-outer {
          padding: 10px;
          border-radius: 3px;
          transition: all 0.8s;
        }

        .post-outer:hover {
          box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
        }

        .dian-sidebar{
          padding: 10px;
          border-radius: 3px;
          box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
        }

        .dian-headline{
          background-color: #000;
          color: #bbb;
          width: 100%;
          margin: 0;
        }
      ]]>
    </b:template-skin>
  </head>

  <body>

    <header>
      <div class="row">
        <div class="col-sm-4">
          <b:section id="dian-header" class="dian-header">
          </b:section>
        </div>

        <div class="col-sm-4">
          <b:section id="dian-middlebar" class="dian-middlebar">
          </b:section>
        </div>

        <div class="col-sm-4">
          <b:section id="dian-sidebar" class="dian-sidebar">
          </b:section>
        </div>
      </div>
    </header>

    <div class="dian-wrapper">
      <div class="row">
        <div class="col-sm-1"></div>
        <div class="col-sm-10">
          <b:section id="dian-content" class="dian-content">
          </b:section>
        </div>
      </div>
    </div>

    <footer>
      <div class="row">
        <div class="col-sm-12">
          <b:section id="dian-footer" class="dian-footer">
          </b:section>
        </div>
      </div>
    </footer>

  </body>

</html>
